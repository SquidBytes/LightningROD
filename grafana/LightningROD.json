            "uid": "c9a74e16-5119-4227-82f9-765d3223da3c"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH charging_intervals AS (\r\n  SELECT\r\n    plug_out_time AS previous_time,\r\n    LEAD(plug_in_time) OVER (ORDER BY plug_out_time) AS next_time\r\n  FROM\r\n    energytransferlogs\r\n)\r\nSELECT\r\n  ROUND(EXTRACT(EPOCH FROM AVG(next_time - previous_time))) AS avg_time_between_charges\r\nFROM\r\n  charging_intervals\r\nWHERE\r\n  next_time IS NOT NULL\r\n  AND previous_time IS NOT NULL;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [
                  {
                    "name": "plug_in_time",
                    "type": "functionParameter"
                  }
                ],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          },
          "table": "energytransferlogs"
        }
      ],
      "title": "Average Time Between Charges",
